{
  "name": "one-study",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/z33p/one-study",
  "author": "z33p <raphael00fellipe@gmail.com>",
  "license": "MIT",
  "scripts": {
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.1.2",
    "cz-customizable": "^6.2.0",
    "husky": "^4.2.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
